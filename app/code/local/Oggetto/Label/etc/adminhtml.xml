<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <label translate="title" module="label">
                                        <title>Label</title>
                                    </label>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
    <layout>
        <updates>
            <label>
                <file>label.xml</file>
            </label>
        </updates>
    </layout>
</config>